{
  "name": "generator-lilly",
  "version": "2.9.0",
  "description": "Personal boilerplate for Martin Herweg, Configurable for Laravel, Craft CMS and Vue",
  "homepage": "https://github.com/martinherweg/lilly",
  "author": {
    "name": "Martin Herweg",
    "email": "info@martinherweg.de",
    "url": "https://martinherweg.de"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "craft",
    "",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "available-versions": "^0.13.6",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.2",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "compare-versions": "^3.4.0",
    "deep-extend": "^0.6.0",
    "dotenv": "^6.0.0",
    "download": "^7.1.0",
    "ejs": "^2.6.1",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-xo": "^0.24.1",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "nsp": "^3.2.1",
    "ora": "^3.0.0",
    "p-iteration": "^1.1.7",
    "prettier": "^1.14.2",
    "regenerator-runtime": "^0.12.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.9.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "command-exists": "^1.2.7",
    "fs-extra": "^7.0.0",
    "progress": "^2.0.0",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "git@github.com:martinherweg/lilly.git",
  "license": "Apache-2.0"
}
